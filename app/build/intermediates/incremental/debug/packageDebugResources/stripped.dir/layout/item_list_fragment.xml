<?xml version="1.0" encoding="utf-8"?>

                                                       
                                                   

    

           

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="@dimen/_7sdp"
        android:layout_marginBottom="@dimen/_7sdp"
        android:background="@drawable/bg_item_import_list_selector"
        android:paddingStart="@dimen/_10sdp"
        android:paddingTop="@dimen/_12sdp"
        android:paddingBottom="@dimen/_9sdp" android:tag="layout/item_list_fragment_0" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">

        <TextView
            android:id="@+id/tv_import_file_name"
            style="@style/Chat.Text.Item.Import.List.Title"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:text="@string/contacts" />


        <TextView
            android:id="@+id/tv_import_file_date_time"
            style="@style/Chat.Text.Item.Import.List.Note"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_5sdp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/tv_import_file_name"
            tools:text="@string/date_import" />


    </androidx.constraintlayout.widget.ConstraintLayout>
         